# Toggle CMA proxy usage (true/false)
CMA_ENABLED=false
# Base URL of the CMA API (e.g., https://api.canadianmortgageapp.com/v1)
CMA_BASE_URL=
# Header name used for the API key (commonly x-api-key)
CMA_AUTH_HEADER=x-api-key
# Your CMA API key
CMA_API_KEY=
# Timeout in ms for CMA requests
CMA_TIMEOUT_MS=8000

# 5-year fixed mortgage APR defaults
RATES_PROVIDER=boc
BOC_SERIES_ID=V122521
DEFAULT_5Y_FIXED_APR=5.00
